{
  "cSpell.userWords": [
    "Digikala",
    "Digipay",
    "Firebolt",
    "Kowsar"
  ],
  "intelephense.files.exclude": [
    "**/.git/**",
    "**/.svn/**",
    "**/.hg/**",
    "**/CVS/**",
    "**/.DS_Store/**",
    "**/node_modules/**",
    "**/bower_components/**",
    "**/.history/**",
    "**/vendor/**/vendor/**"
  ],
  "languageserver": {
    "sqls": {
      "command": "sqls",
      "filetypes": [
        "sql"
      ],
      "shell": true
    },
    "psalmls": {
      "command": "psalm",
      "args": [
        "--language-server"
      ],
      "filetypes": [
        "php"
      ],
      "rootPatterns": [
        "psalm.xml",
        "psalm.xml.dist"
      ],
      "requireRootPattern": false
    }
  },
  "coc.preferences.useQuickfixForLocations": true
}
